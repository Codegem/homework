== true){\n          setloading(true);\n          setTimeout(() => {\n            setloading(false);\n            return result.json()\n          }, 3000);\n        }else{\n          return result.json()\n        }\n      })\n      .then((data) => {\n        setdata(data.results);\n      })\n      .catch(function(error){\n        console.log('something went completely wrong')\n        return; // atvirai tai nezinau ar padetu nes error negavau ^^\n      })\n    }\n  },[searchText]);\n\n  const inputHandler = (e) => {\n    const text = e.target.value;\n    setsearchText(text);\n    setclicked(false);\n    setinputLength(text.length);\n  }\n\n  return (\n    <div>\n      <header>\n        <div className=\"search-content\">\n          <div className=\"search-wrap\">\n            <Movie className=\"movie-icon\"/>\n            <input \n              type=\"text\" \n              placeholder=\"Enter Movie Name\" \n              id=\"search\" \n              onChange={inputHandler}\n              value={searchText} \n            />\n          </div>\n          <Search className=\"search-icon\"/>\n        </div>\n          <SearchRez data={data} inputLength={inputLength} selectionHandler = {searchText => setsearchText(searchText)} inputTxt={searchText} clicked={clicked} selected={clicked => setclicked(clicked)}/>\n      </header>\n      {loading === true ? <img className=\"loading\" src={Loading}/> : <MovieInfo data={data} clicked={clicked} selected_value={searchText}/> }\n    </div>\n  )\n}\n","/home/evaldas/Desktop/uiacademyTask/react-movies/src/api/MovieSource.js",[],"/home/evaldas/Desktop/uiacademyTask/react-movies/src/components/search/SearchResults.jsx",[],"/home/evaldas/Desktop/uiacademyTask/react-movies/src/components/movie_info/MovieInfo.jsx",["45"],"import React from 'react'\nimport Noimg from '../header/icons/noimage.png'\n\nexport default function MovieInfo(props) {\n\n  const movie = props.data;\n\n  console.log(props.selected_value)\n  if(props.clicked === true){\n    return (\n        <div className=\"movie\">\n          {movie.map((movie, index) => {\n            if(movie.original_title === props.selected_value){\n              const path = movie.poster_path;\n              const img_src = \"https://image.tmdb.org/t/p/original/\" + path;\n                return(\n                  <div className=\"movie__card\" key={index}>\n                    <h1>{movie.title}</h1>\n                    <p>{movie.overview}</p>\n                    {(movie.poster_path !== null)?<img src={img_src} alt=\"img\"/>:<img src={Noimg} alt=\"no-image\"/>}\n                    <section>\n                      <p>Popularity: <span>{movie.popularity}</span></p>\n                      <p>Vote Average: <span>{movie.vote_average}</span></p>\n                      <p>Language: <span>{movie.original_language}</span></p>\n                      <p>Released: <span>{movie.release_date}</span></p>\n                    </section>\n                  </div>\n                );\n            }else{\n              return null;\n            }\n            })\n          }\n        </div>\n    )  \n  }\n  else{\n    return null;\n  }\n}\n",{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","severity":1,"message":"51","line":43,"column":5,"nodeType":"52","endLine":43,"endColumn":17,"suggestions":"53"},{"ruleId":"54","severity":1,"message":"55","line":70,"column":27,"nodeType":"56","endLine":70,"endColumn":67},{"ruleId":"57","severity":1,"message":"58","line":20,"column":82,"nodeType":"56","endLine":20,"endColumn":115},"no-native-reassign",["59"],"no-negated-in-lhs",["60"],"react-hooks/exhaustive-deps","React Hook React.useEffect has a missing dependency: 'clicked'. Either include it or remove the dependency array.","ArrayExpression",["61"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","jsx-a11y/img-redundant-alt","Redundant alt attribu